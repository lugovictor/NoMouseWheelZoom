{
  "manifest_version": 2,

  "name": "NoMouseWheelZoom",
  "description": "This extension disables the annoying zoom shortcut Ctrl+ Mousewheel.",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentscript.js"]
    }
  ],
  "web_accessible_resources": [
    "jquery.min.js",
    "NoMouseWheelZoom.js"
  ]
}
